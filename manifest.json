{
	"name": "MyCSS",
	"description": "",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [
		"storage",
        "tabs"
	],
	"action": {
		"default_title": "Testing",
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts":[{
		"run_at":"document_start",
		"matches": ["http://*/*","https://*/*"],
		"js": ["loader.js"]
	}]
}